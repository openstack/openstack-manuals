msgstr "Java"

msgid "JavaScript"
msgstr "JavaScript"

msgid "JavaScript Object Notation (JSON)"
msgstr "JavaScript Object Notation (JSON)"

msgid "Juno"
msgstr "Juno"

msgid "Kickstart"
msgstr "Kickstart"

msgid "Kilo"
msgstr "Kilo"

msgid "Launchpad"
msgstr "Launchpad"

